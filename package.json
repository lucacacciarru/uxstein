{
  "name": "frankenstein",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/lato": "^4.5.1",
    "@fontsource/nunito": "^4.5.4",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-grid-layout": "^1.3.0",
    "framer-motion": "^5.6.0",
    "html2canvas": "^1.4.1",
    "i18next": "^21.6.11",
    "jspdf": "^2.5.1",
    "polished": "^4.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.3",
    "react-i18next": "^11.15.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "typescript": "^4.5.5",
    "use-debounce": "^7.0.1",
    "web-vitals": "^2.1.4",
    "wretch": "^1.7.9"
  },
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run start:json-server\"",
    "start": "react-scripts start",
    "start:json-server": "ts-node scripts/json-server.ts",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:ci": "npm run test:coverage -- --testResultsProcessor=\"jest-junit\"",
    "test:watch": "react-scripts test --watchAll=true",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "prettify": "npx prettier --write \"**/*.ts(x|)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "json-server": "^0.17.0",
    "ts-node": "^10.6.0"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80,
        "branches": 80,
        "functions": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/**/*.ts",
      "<rootDir>/src/**/**/*.tsx",
      "!<rootDir>/src/**/**/*.d.ts"
    ]
  }
}
